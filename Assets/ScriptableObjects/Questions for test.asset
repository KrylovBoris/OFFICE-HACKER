  generalQuestions:
  - text: "\u041A\u0430\u043A\u0438\u043C\u0438 \u043A\u0430\u0447\u0435\u0441\u0442\u0432\u0430\u043C\u0438
      \u0436\u0435\u0440\u0442\u0432\u044B \u043C\u043E\u0436\u0435\u0442 \u0432\u043E\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u044C\u0441\u044F
      \u0437\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A,
      \u0441\u043E\u0432\u0435\u0440\u0448\u0430\u044E\u0449\u0438\u0439 \u0441\u043E\u0446\u0438\u043E\u0438\u043D\u0436\u0435\u043D\u0435\u0440\u043D\u0443\u044E
      \u0430\u0442\u0430\u043A\u0443?"
    correctAnswerVariant: "\u0416\u0435\u043B\u0430\u043D\u0438\u0435\u043C \u043F\u043E\u043C\u043E\u0447\u044C,
      \u043F\u043E\u0442\u0440\u0435\u0431\u043D\u043E\u0441\u0442\u044C\u044E \u0432
      \u0434\u0435\u043D\u044C\u0433\u0430\u0445 \u0438 \u043D\u0435\u0434\u043E\u0431\u0440\u043E\u0441\u043E\u0432\u0435\u0441\u0442\u043D\u043E\u0441\u0442\u044C\u044E."
    incorrectAnswerVariants:
    - answer: "\u0416\u0435\u043B\u0430\u043D\u0438\u0435 \u043F\u043E\u043C\u043E\u0447\u044C."
      clarification: "\u0417\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A
        \u043C\u043E\u0436\u0435\u0442 \u0432\u043E\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u044C\u0441\u044F
        \u043D\u0435 \u0442\u043E\u043B\u044C\u043A\u043E \u0436\u0435\u043B\u0430\u043D\u0438\u0435\u043C
        \u0436\u0435\u0440\u0442\u0432\u044B \u043F\u043E\u043C\u043E\u0447\u044C."
    - answer: "\u041F\u043E\u0442\u0440\u0435\u0431\u043D\u043E\u0441\u0442\u044C
        \u0432 \u0434\u0435\u043D\u044C\u0433\u0430\u0445."
      clarification: "\u0417\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A
        \u043C\u043E\u0436\u0435\u0442 \u0432\u043E\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u044C\u0441\u044F
        \u043D\u0435 \u0442\u043E\u043B\u044C\u043A\u043E \u043F\u043E\u0442\u0440\u0435\u0431\u043D\u043E\u0441\u0442\u044C\u044E
        \u0436\u0435\u0440\u0442\u0432\u044B \u0432 \u0434\u0435\u043D\u044C\u0433\u0430\u0445."
    - answer: "\u041D\u0435\u0434\u043E\u0431\u0440\u043E\u0441\u043E\u0432\u0435\u0441\u0442\u043D\u043E\u0441\u0442\u044C."
      clarification: "\u0417\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A
        \u043C\u043E\u0436\u0435\u0442 \u0432\u043E\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u044C\u0441\u044F
        \u043D\u0435 \u0442\u043E\u043B\u044C\u043A\u043E \u043D\u0435\u0434\u043E\u0431\u0440\u043E\u0441\u043E\u0432\u0435\u0441\u0442\u043D\u043E\u0441\u0442\u044C\u044E
        \u0436\u0435\u0440\u0442\u0432\u044B."
  - text: "\u0424\u0430\u0439\u043B\u044B \u0441 \u043A\u0430\u043A\u0438\u043C \u0440\u0430\u0441\u0448\u0438\u0440\u0435\u043D\u0438\u0435\u043C
      100% \u0431\u0435\u0437\u043E\u043F\u0430\u0441\u043D\u044B?"
    correctAnswerVariant: .bmp
    incorrectAnswerVariants:
    - answer: .pdf
      clarification: "\u041D\u0435\u0441\u043C\u043E\u0442\u0440\u044F \u043D\u0430
        \u043F\u043E\u043F\u0443\u043B\u044F\u0440\u043D\u043E\u0441\u0442\u044C,
        PDF-\u0444\u043E\u0440\u043C\u0430\u0442 \u043E\u043F\u0430\u0441\u0435\u043D,
        \u0442\u0430\u043A \u043A\u0430\u043A \u043C\u043E\u0436\u0435\u0442 \u0441\u043E\u0434\u0435\u0440\u0436\u0430\u0442\u044C
        \u0432\u0438\u0440\u0443\u0441\u044B."
    - answer: .pptx
      clarification: "\u0414\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u044B Microsoft
        Office \u043C\u043E\u0433\u0443\u0442 \u0441\u043E\u0434\u0435\u0440\u0436\u0430\u0442\u044C
        \u0432\u0440\u0435\u0434\u043E\u043D\u043E\u0441\u043D\u044B\u0439 \u043A\u043E\u0434,
        \u043D\u0430\u043F\u0438\u0441\u0430\u043D\u043D\u044B\u0439 \u043D\u0430
        \u044F\u0437\u044B\u043A\u0435 \u043C\u0430\u043A\u0440\u043E\u0441\u043E\u0432."
    - answer: .exe
      clarification: "\u0418\u0441\u043F\u043E\u043B\u043D\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0435
        \u0444\u0430\u0439\u043B\u044B \u043C\u043E\u0433\u0443\u0442 \u0441\u043E\u0434\u0435\u0440\u0436\u0430\u0442\u044C
        \u0432\u0440\u0435\u0434\u043E\u043D\u043E\u0441\u043D\u044B\u0439 \u043A\u043E\u0434."
  - text: "\u0413\u0434\u0435 \u043C\u043E\u0436\u043D\u043E \u043D\u0430\u0434\u0451\u0436\u043D\u043E
      \u0445\u0440\u0430\u043D\u0438\u0442\u044C \u043F\u0430\u0440\u043E\u043B\u0438?"
    correctAnswerVariant: "\u0412 \u043C\u0435\u043D\u0435\u0434\u0436\u0435\u0440\u0435
      \u043F\u0430\u0440\u043E\u043B\u0435\u0439."
    incorrectAnswerVariants:
    - answer: "\u041D\u0430 \u0445\u043E\u0440\u043E\u0448\u043E \u0441\u043F\u0440\u044F\u0442\u0430\u043D\u043D\u043E\u0439
        \u0431\u0443\u043C\u0430\u0436\u043A\u0435."
      clarification: "\u0412 \u043E\u0444\u0438\u0441\u0435 \u0441\u043B\u0438\u0448\u043A\u043E\u043C
        \u043C\u043D\u043E\u0433\u043E \u043B\u044E\u0434\u0435\u0439, \u0432\u044B
        \u043D\u0435 \u043C\u043E\u0436\u0435\u0442\u0435 \u0433\u0430\u0440\u0430\u043D\u0442\u0438\u0440\u043E\u0432\u0430\u0442\u044C
        \u0442\u043E, \u0447\u0442\u043E \u043D\u0438\u043A\u0442\u043E \u0412\u0430\u0448\u0443
        \u0431\u0443\u043C\u0430\u0436\u043A\u0443 \u043D\u0435 \u043D\u0430\u0439\u0434\u0451\u0442."
    - answer: "\u0412 \u043F\u0440\u043E\u0441\u0442\u043E\u043C \u0442\u0435\u043A\u0441\u0442\u043E\u0432\u043E\u043C
        \u0444\u0430\u0439\u043B\u0435 \u043D\u0430 \u043A\u043E\u043C\u043F\u044C\u044E\u0442\u0435\u0440\u0435."
      clarification: "\u0415\u0441\u043B\u0438 \u0444\u0430\u0439\u043B \u043D\u0430
        \u043A\u043E\u043C\u043F\u044C\u044E\u0442\u0435\u0440\u0435 \u0441\u0430\u043C
        \u043D\u0435 \u0437\u0430\u0449\u0438\u0449\u0451\u043D \u043F\u0430\u0440\u043E\u043B\u0435\u043C
        \u0438\u043B\u0438 \u043D\u0435 \u0437\u0430\u0448\u0438\u0444\u0440\u043E\u0432\u0430\u043D,
        \u043D\u0430\u0434\u0451\u0436\u043D\u044B\u043C \u043C\u0435\u0441\u0442\u043E\u043C
        \u0434\u043B\u044F \u0445\u0440\u0430\u043D\u0435\u043D\u0438\u044F \u043F\u0430\u0440\u043E\u043B\u044F
        \u043E\u043D \u043D\u0435 \u043C\u043E\u0436\u0435\u0442 \u0431\u044B\u0442\u044C."
    - answer: "\u041D\u0438\u0433\u0434\u0435."
      clarification: "\u041D\u0443, \u044D\u0442\u043E \u043F\u0435\u0440\u0435\u0431\u043E\u0440.
        \u0421\u0432\u043E\u0439 \u043F\u0430\u0440\u043E\u043B\u044C \u043D\u0435\u043F\u043B\u043E\u0445\u043E
        \u0445\u0440\u0430\u043D\u0438\u0442\u044C \u0445\u043E\u0442\u044F \u0431\u044B
        \u0432 \u0441\u0432\u043E\u0435\u0439 \u0433\u043E\u043B\u043E\u0432\u0435.
        :)"
  - text: "\u041A\u0430\u043A\u043E\u0439 \u0438\u0437 \u044D\u0442\u0438\u0445 \u043F\u0430\u0440\u043E\u043B\u0435\u0439
      \u0412\u044B \u043F\u043E\u0441\u043E\u0432\u0435\u0442\u0443\u0435\u0442\u0435?"
    correctAnswerVariant: "\u041D\u0438 \u043E\u0434\u0438\u043D \u0438\u0437 \u043F\u0440\u0435\u0434\u043B\u043E\u0436\u0435\u043D\u043D\u044B\u0445."
    incorrectAnswerVariants:
    - answer: qwerty12345
      clarification: "\u041E\u0447\u0435\u043D\u044C \u043F\u043E\u043F\u0443\u043B\u044F\u0440\u043D\u044B\u0439
        \u043F\u0430\u0440\u043E\u043B\u044C."
    - answer: MarinaIvanovna89
      clarification: "\u041D\u0435 \u0441\u0442\u043E\u0438\u0442 \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u044C
        \u0432 \u043A\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u043F\u0430\u0440\u043E\u043B\u044F
        \u0441\u043E\u0431\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0435 \u0438\u043C\u044F,
        \u0444\u0430\u043C\u0438\u043B\u0438\u044E, \u043E\u0442\u0447\u0435\u0441\u0442\u0432\u043E
        \u0438\u043B\u0438 \u0433\u043E\u0434 \u0440\u043E\u0436\u0434\u0435\u043D\u0438\u044F."
    - answer: 6H&^k(
      clarification: "\u0418\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u043D\u0438\u0435
        \u0446\u0438\u0444\u0440 \u0438 \u0440\u0430\u0437\u043B\u0438\u0447\u043D\u044B\u0445
        \u0441\u0438\u043C\u0432\u043E\u043B\u043E\u0432 \u0432 \u043F\u0430\u0440\u043E\u043B\u0435
        \u2014 \u0445\u043E\u0440\u043E\u0448\u0430\u044F \u0438\u0434\u0435\u044F,
        \u043D\u043E \u044D\u0442\u043E\u0442 \u043F\u0430\u0440\u043E\u043B\u044C
        \u0441\u043B\u0438\u0448\u043A\u043E\u043C \u043A\u043E\u0440\u043E\u0442\u043A\u0438\u0439."
  - text: "\u041D\u0430\u0437\u043E\u0432\u0438\u0442\u0435 \u0441\u0430\u043C\u0443\u044E
      \u0443\u044F\u0437\u0432\u0438\u043C\u0443\u044E \u0447\u0430\u0441\u0442\u044C
      \u0438\u043D\u0444\u043E\u0440\u043C\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0439
      \u0441\u0438\u0441\u0442\u0435\u043C\u044B."
    correctAnswerVariant: "\u041F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u0438."
    incorrectAnswerVariants:
    - answer: "\u041A\u043E\u043C\u043F\u044C\u044E\u0442\u0435\u0440\u044B."
      clarification: "\u041A\u043E\u043C\u043F\u044C\u044E\u0442\u0435\u0440\u044B
        \u0447\u0430\u0441\u0442\u043E \u0437\u0430\u0449\u0438\u0449\u0435\u043D\u044B
        \u043F\u0430\u0440\u043E\u043B\u044F\u043C\u0438, \u0444\u0430\u0439\u0440\u0432\u043E\u043B\u0430\u043C\u0438
        \u0438 \u0430\u043D\u0442\u0438\u0432\u0438\u0440\u0443\u0441\u043D\u044B\u043C\u0438
        \u043F\u0440\u043E\u0433\u0440\u0430\u043C\u043C\u0430\u043C\u0438."
    - answer: "\u0421\u0435\u0440\u0432\u0435\u0440\u044B."
      clarification: "\u0421\u0435\u0440\u0432\u0435\u0440\u044B \u0437\u0430\u0447\u0430\u0441\u0442\u0443\u044E
        \u043D\u0430\u0434\u0451\u0436\u043D\u043E \u0437\u0430\u0449\u0438\u0449\u0435\u043D\u044B
        \u0441 \u043F\u043E\u043C\u043E\u0449\u044C\u044E."
    - answer: "\u041A\u0430\u043D\u0430\u043B\u044B \u043F\u0435\u0440\u0435\u0434\u0430\u0447\u0438
        \u0438\u043D\u0444\u043E\u0440\u043C\u0430\u0446\u0438\u0438."
      clarification: "\u041D\u0435\u0441\u043C\u043E\u0442\u0440\u044F \u043D\u0430
        \u0442\u043E, \u0447\u0442\u043E \u043F\u043E\u0445\u0438\u0442\u0438\u0442\u044C
        \u0438\u043D\u0444\u043E\u0440\u043C\u0430\u0446\u0438\u044E \u0438\u0437
        \u043A\u0430\u043D\u0430\u043B\u043E\u0432 \u043F\u0435\u0440\u0435\u0434\u0430\u0447\u0438
        \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E, \u044D\u0442\u043E \u0442\u0440\u0435\u0431\u0443\u0435\u0442
        \u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043A\u043E\u0433\u043E \u043F\u043E\u0434\u043A\u043B\u044E\u0447\u0435\u043D\u0438\u044F
        \u043A \u043A\u0430\u043D\u0430\u043B\u0443 \u0441\u0432\u044F\u0437\u0438.
        \u0418 \u0434\u0430\u0436\u0435 \u043F\u043E\u0441\u043B\u0435 \u043F\u043E\u0445\u0438\u0449\u0435\u043D\u0438\u044F
        \u0437\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A\u0430\u043C
        \u0442\u0440\u0435\u0431\u0443\u0435\u0442\u0441\u044F \u0440\u0430\u0441\u0448\u0438\u0444\u0440\u043E\u0432\u0430\u0442\u044C
        \u043F\u043E\u0445\u0438\u0449\u0435\u043D\u043D\u0443\u044E \u0438\u043D\u0444\u043E\u0440\u043C\u0430\u0446\u0438\u044E."
  - text: "\u041D\u0443\u0436\u0435\u043D \u043B\u0438 \u043A\u043E\u043C\u043F\u0430\u043D\u0438\u0438
      \u0448\u0440\u0435\u0434\u0435\u0440?"
    correctAnswerVariant: "\u041B\u044E\u0431\u043E\u0439 \u043A\u043E\u043C\u043F\u0430\u043D\u0438\u0438
      \u043D\u0435\u043E\u0431\u0445\u043E\u0434\u0438\u043C \u0448\u0440\u0435\u0434\u0435\u0440
      \u0434\u043B\u044F \u0443\u043D\u0438\u0447\u0442\u043E\u0436\u0435\u043D\u0438\u044F
      \u0432\u0430\u0436\u043D\u044B\u0445 \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u043E\u0432."
    incorrectAnswerVariants:
    - answer: "\u041D\u0435 \u043D\u0443\u0436\u0435\u043D. \u0414\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u044B
        \u043C\u043E\u0436\u043D\u043E \u0443\u043D\u0438\u0447\u0442\u043E\u0436\u0430\u0442\u044C
        \u0432\u0440\u0443\u0447\u043D\u0443\u044E."
      clarification: "\u0423\u043D\u0438\u0447\u0442\u043E\u0436\u0435\u043D\u0438\u0435
        \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u043E\u0432 \u0432\u0440\u0443\u0447\u043D\u0443\u044E
        \u043D\u0435\u043D\u0430\u0434\u0451\u0436\u043D\u043E, \u0437\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A
        \u043C\u043E\u0436\u0435\u0442 \u0432\u043E\u0441\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u044C
        \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442 \u0438\u0437 \u0444\u0440\u0430\u0433\u043C\u0435\u043D\u0442\u043E\u0432,
        \u0435\u0441\u043B\u0438 \u043E\u043D\u0438 \u043D\u0435\u0434\u043E\u0441\u0442\u0430\u0442\u043E\u0447\u043D\u043E
        \u043C\u0435\u043B\u043A\u0438\u0435."
    - answer: "\u041D\u0435 \u043D\u0443\u0436\u0435\u043D. \u0423\u043D\u0438\u0447\u0442\u043E\u0436\u0430\u0442\u044C
        \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u044B \u043D\u0435\u043E\u0431\u044F\u0437\u0430\u0442\u0435\u043B\u044C\u043D\u043E."
      clarification: "\u041B\u044E\u0431\u0430\u044F \u043A\u043E\u043C\u043F\u0430\u043D\u0438\u044F
        \u043C\u043E\u0436\u0435\u0442 \u043F\u043E\u0442\u0435\u0440\u043F\u0435\u0442\u044C
        \u0443\u0431\u044B\u0442\u043A\u0438, \u0435\u0441\u043B\u0438 \u0432 \u0440\u0443\u043A\u0438
        \u0437\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A\u043E\u0432
        \u043F\u043E\u043F\u0430\u0434\u0443\u0442 \u0435\u0451 \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u044B."
    - answer: "\u041D\u0443\u0436\u0435\u043D \u0442\u043E\u043B\u044C\u043A\u043E
        \u0431\u043E\u043B\u044C\u0448\u0438\u043C \u043A\u043E\u043C\u043F\u0430\u043D\u0438\u044F\u043C."
      clarification: "\u0414\u0430\u0436\u0435 \u0438\u043D\u0434\u0438\u0432\u0438\u0434\u0443\u0430\u043B\u044C\u043D\u044B\u0439
        \u043F\u0440\u0435\u0434\u043F\u0440\u0438\u043D\u0438\u043C\u0430\u0442\u0435\u043B\u044C
        \u043C\u043E\u0436\u0435\u0442 \u043F\u043E\u0442\u0435\u0440\u043F\u0435\u0442\u044C
        \u0443\u0431\u044B\u0442\u043A\u0438, \u0435\u0441\u043B\u0438 \u0435\u0433\u043E
        \u0432\u0430\u0436\u043D\u044B\u0435 \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u044B
        \u043F\u043E\u043F\u0430\u0434\u0443\u0442 \u0432 \u0440\u0443\u043A\u0438
        \u0437\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A\u043E\u0432."
  - text: "\u041A\u0430\u043A\u0438\u0435 \u0438\u0437 \u044D\u0442\u0438\u0445 \u0444\u0430\u0439\u043B\u043E\u0432
      \u0442\u043E\u0447\u043D\u043E \u0431\u0435\u0437\u043E\u043F\u0430\u0441\u043D\u044B?"
    correctAnswerVariant: trojan.mp3
    incorrectAnswerVariants:
    - answer: absolutely_safe_script.bat
      clarification: ".bat-\u0444\u0430\u0439\u043B\u044B \u0441\u043E\u0434\u0435\u0440\u0436\u0430\u0442
        \u0438\u043D\u0441\u0442\u0440\u0443\u043A\u0446\u0438\u0438 \u0434\u043B\u044F
        \u043A\u043E\u043C\u0430\u043D\u0434\u043D\u043E\u0439 \u0441\u0442\u0440\u043E\u043A\u0438,
        \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u043C\u043E\u0433\u0443\u0442
        \u0441\u043E\u0434\u0435\u0440\u0436\u0430\u0442\u044C \u0432\u0440\u0435\u0434\u043E\u043D\u043E\u0441\u043D\u044B\u0439
        \u043A\u043E\u0434."
    - answer: foo.reg
      clarification: ".reg-\u0444\u0430\u0439\u043B\u044B \u0441\u043E\u0437\u0434\u0430\u043D\u044B
        \u0434\u043B\u044F \u0432\u043D\u0435\u0441\u0435\u043D\u0438\u044F \u0438\u0437\u043C\u0435\u043D\u0435\u043D\u0438\u0439
        \u0432 \u0440\u0435\u0435\u0441\u0442\u0440 Windows, \u0430 \u043D\u0435\u043A\u043E\u0442\u043E\u0440\u044B\u0435
        \u0442\u0430\u043A\u0438\u0435 \u0438\u0437\u043C\u0435\u043D\u0435\u043D\u0438\u044F
        \u043C\u043E\u0433\u0443\u0442 \u043F\u0440\u0438\u0447\u0438\u043D\u0438\u0442\u044C
        \u0432\u0440\u0435\u0434 \u0412\u0430\u0448\u0435\u0439 \u0441\u0438\u0441\u0442\u0435\u043C\u0435."
    - answer: otchyot.pdf
      clarification: "\u041D\u0435\u0441\u043C\u043E\u0442\u0440\u044F \u043D\u0430
        \u043F\u043E\u043F\u0443\u043B\u044F\u0440\u043D\u043E\u0441\u0442\u044C,
        PDF-\u0444\u043E\u0440\u043C\u0430\u0442 \u043E\u043F\u0430\u0441\u0435\u043D,
        \u0442\u0430\u043A \u043A\u0430\u043A \u043C\u043E\u0436\u0435\u0442 \u0441\u043E\u0434\u0435\u0440\u0436\u0430\u0442\u044C
        \u0432\u0438\u0440\u0443\u0441\u044B."
  - text: "\u0427\u0442\u043E \u043C\u043D\u0435 \u0434\u0435\u043B\u0430\u0442\u044C
      \u0441 \u043D\u0430\u0439\u0434\u0435\u043D\u043D\u043E\u0439 \u043D\u0430
      \u043F\u0430\u0440\u043A\u043E\u0432\u043A\u0435 \u0444\u043B\u0435\u0448\u043A\u043E\u0439?"
    correctAnswerVariant: "\u041F\u0435\u0440\u0435\u0434\u0430\u0442\u044C \u0441\u0438\u0441\u0442\u0435\u043C\u043D\u043E\u043C\u0443
      \u0430\u0434\u043C\u0438\u043D\u0438\u0441\u0442\u0440\u0430\u0442\u043E\u0440\u0443."
    incorrectAnswerVariants:
    - answer: "\u041F\u0440\u043E\u0432\u0435\u0440\u0438\u0442\u044C \u0441\u043E\u0434\u0435\u0440\u0436\u0438\u043C\u043E\u0435
        \u0444\u043B\u0435\u0448\u043A\u0438 \u0441\u0430\u043C\u043E\u0439. \u041C\u043E\u0436\u0435\u0442,
        \u0442\u0430\u043C \u0431\u0443\u0434\u0435\u0442 \u0437\u0430\u0446\u0435\u043F\u043A\u0430
        \u0433\u0434\u0435 \u0438\u0441\u043A\u0430\u0442\u044C \u0435\u0451 \u0445\u043E\u0437\u044F\u0438\u043D\u0430."
      clarification: "\u041D\u0438 \u0432 \u043A\u043E\u0435\u043C \u0441\u043B\u0443\u0447\u0430\u0435
        \u043D\u0435\u043B\u044C\u0437\u044F \u0432\u0441\u0442\u0430\u0432\u043B\u044F\u0442\u044C
        \u0432 \u043A\u043E\u0440\u043F\u043E\u0440\u0430\u0442\u0438\u0432\u043D\u044B\u0435
        \u043A\u043E\u043C\u043F\u044C\u044E\u0442\u0435\u0440\u044B \u043D\u0435\u0438\u0437\u0432\u0435\u0441\u0442\u043D\u044B\u0435
        \u0444\u043B\u0435\u0448\u043A\u0438. \u041E\u043D\u0438 \u043C\u043E\u0433\u0443\u0442
        \u0441\u043E\u0434\u0435\u0440\u0436\u0430\u0442\u044C \u0432\u0438\u0440\u0443\u0441\u044B."
  - text: "\u0427\u0442\u043E \u043D\u0443\u0436\u043D\u043E \u0441\u0434\u0435\u043B\u0430\u0442\u044C,
      \u0447\u0442\u043E\u0431\u044B \u043C\u043E\u0439 \u043F\u0430\u0440\u043E\u043B\u044C
      \u043D\u0435 \u043F\u043E\u0434\u0441\u043C\u043E\u0442\u0440\u0435\u043B\u0438?"
    correctAnswerVariant: "\u0412\u0441\u0435\u0433\u0434\u0430 \u043E\u0431\u0440\u0430\u0449\u0430\u0442\u044C
      \u0432\u043D\u0438\u043C\u0430\u043D\u0438\u0435, \u043A\u0442\u043E \u0441\u0442\u043E\u0438\u0442
      \u0437\u0430 \u0441\u043F\u0438\u043D\u043E\u0439, \u0432\u043E \u0432\u0440\u0435\u043C\u044F
      \u0432\u0432\u043E\u0434\u0430 \u043F\u0430\u0440\u043E\u043B\u044F."
    incorrectAnswerVariants:
    - answer: "\u0412\u044B\u043A\u043B\u044E\u0447\u0430\u0442\u044C \u043C\u043E\u043D\u0438\u0442\u043E\u0440
        \u0432\u043E \u0432\u0440\u0435\u043C\u044F \u0432\u0432\u043E\u0434\u0430
        \u043F\u0430\u0440\u043E\u043B\u044F."
      clarification: "\u0412\u044B\u043A\u043B\u044E\u0447\u0435\u043D\u043D\u044B\u0439
        \u043C\u043E\u043D\u0438\u0442\u043E\u0440 \u043D\u0438\u043A\u0430\u043A
        \u043D\u0435 \u043C\u0435\u0448\u0430\u0435\u0442 \u0437\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A\u0443
        \u0441\u043B\u0435\u0434\u0438\u0442\u044C \u0437\u0430 \u0440\u0443\u043A\u0430\u043C\u0438."
    - answer: "\u0412\u0432\u043E\u0434\u0438\u0442\u044C \u043F\u0430\u0440\u043E\u043B\u044C
        \u0431\u044B\u0441\u0442\u0440\u0435\u0435."
      clarification: "\u0417\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A
        \u043C\u043E\u0436\u0435\u0442 \u0441\u043D\u0438\u043C\u0430\u0442\u044C
        \u0412\u0430\u0441 \u043D\u0430 \u043A\u0430\u043C\u0435\u0440\u0443, \u0447\u0442\u043E\u0431\u044B
        \u043F\u043E \u0437\u0430\u043F\u0438\u0441\u0438 \u0432\u043E\u0441\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u044C
        \u043F\u0430\u0440\u043E\u043B\u044C, \u043A\u043E\u0442\u043E\u0440\u044B\u0439
        \u0412\u044B \u0432\u0432\u043E\u0434\u0438\u0442\u0435."
    - answer: "\u041D\u0438\u043A\u0442\u043E \u043D\u0435 \u0441\u0442\u0430\u043D\u0435\u0442
        \u043F\u043E\u0434\u0441\u043C\u0430\u0442\u0440\u0438\u0432\u0430\u0442\u044C
        \u043F\u0430\u0440\u043E\u043B\u044C."
      clarification: "\u041F\u043E\u0434\u0441\u043C\u0430\u0442\u0440\u0438\u0432\u0430\u043D\u0438\u0435
        \u043F\u0430\u0440\u043E\u043B\u0435\u0439 \u0432 \u043E\u0431\u0449\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0445
        \u043C\u0435\u0441\u0442\u0430\u0445 \u0438 \u0447\u0435\u0440\u0435\u0437
        \u0432\u0435\u0431-\u043A\u0430\u043C\u0435\u0440\u0443 \u2014 \u0440\u0430\u0441\u043F\u0440\u043E\u0441\u0442\u0440\u0430\u043D\u0451\u043D\u043D\u0430\u044F
        \u0442\u0435\u0445\u043D\u0438\u043A\u0430 \u0430\u0442\u0430\u043A\u0438."
  - text: "\u0427\u0442\u043E \u043C\u043E\u0438\u043C \u0441\u043E\u0442\u0440\u0443\u0434\u043D\u0438\u043A\u0430\u043C
      \u043D\u0443\u0436\u043D\u043E \u0441\u0434\u0435\u043B\u0430\u0442\u044C,
      \u043A\u043E\u0433\u0434\u0430 \u043D\u0430 \u043D\u0438\u0445 \u043F\u044B\u0442\u0430\u044E\u0442\u0441\u044F
      \u043D\u0430\u0434\u0430\u0432\u0438\u0442\u044C \u0447\u0443\u0436\u0438\u043C
      \u0430\u0432\u0442\u043E\u0440\u0438\u0442\u0435\u0442\u043E\u043C?"
    correctAnswerVariant: "\u041D\u0435 \u043F\u043E\u0434\u0434\u0430\u0432\u0430\u0442\u044C\u0441\u044F
      \u043D\u0430 \u043F\u0440\u043E\u0432\u043E\u043A\u0430\u0446\u0438\u044E \u0438
      \u0441\u043E\u043E\u0431\u0449\u0438\u0442\u044C \u0432 \u0441\u043B\u0443\u0436\u0431\u0443
      \u0431\u0435\u0437\u043E\u043F\u0430\u0441\u043D\u043E\u0441\u0442\u0438."
    incorrectAnswerVariants:
    - answer: "\u041D\u0443\u0436\u043D\u043E \u043F\u0440\u043E\u0441\u0442\u043E
        \u0434\u0430\u0442\u044C \u043C\u0430\u043D\u0438\u043F\u0443\u043B\u044F\u0442\u043E\u0440\u0443
        \u0442\u043E, \u0447\u0442\u043E \u043E\u043D \u0445\u043E\u0447\u0435\u0442.
        \u0412\u0430\u0441 \u044D\u0442\u043E \u043D\u0435 \u043A\u0430\u0441\u0430\u0435\u0442\u0441\u044F."
      clarification: "\u0427\u0430\u0441\u0442\u043E \u043C\u0430\u043D\u0438\u043F\u0443\u043B\u044F\u0442\u043E\u0440
        \u0445\u043E\u0447\u0435\u0442 \u043E\u0442 \u0412\u0430\u0441 \u0447\u0435\u0433\u043E-\u0442\u043E,
        \u043E\u0442 \u0447\u0435\u0433\u043E \u043C\u043E\u0436\u0435\u0442 \u043F\u043E\u0441\u0442\u0440\u0430\u0434\u0430\u0442\u044C
        \u0412\u0430\u0448\u0430 \u043A\u043E\u043C\u043F\u0430\u043D\u0438\u044F,
        \u0430 \u0412\u044B \u2014 \u043B\u0438\u0448\u0438\u0442\u044C\u0441\u044F
        \u0440\u0430\u0431\u043E\u0442\u044B."
    - answer: "\u041D\u0430\u043F\u0430\u0441\u0442\u044C \u0438 \u0437\u0430\u0434\u0435\u0440\u0436\u0430\u0442\u044C
        \u0433\u043E\u0440\u0435-\u043C\u0430\u043D\u0438\u043F\u0443\u043B\u044F\u0442\u043E\u0440\u0430
        \u043D\u0430 \u043C\u0435\u0441\u0442\u0435."
      clarification: "\u041D\u0430\u043F\u0430\u0434\u0435\u043D\u0438\u0435 \u043D\u0430
        \u043C\u0430\u043D\u0438\u043F\u0443\u043B\u044F\u0442\u043E\u0440\u0430
        \u2014 \u043D\u0435 \u043B\u0443\u0447\u0448\u0430\u044F \u0438\u0434\u0435\u044F.
        \u041E\u043D \u043C\u043E\u0436\u0435\u0442 \u043E\u043A\u0430\u0437\u0430\u0442\u044C\u0441\u044F
        \u0441\u0438\u043B\u044C\u043D\u0435\u0435 \u0412\u0430\u0441."
  - text: "\u0427\u0442\u043E \u0438\u0437 \u0441\u043B\u0435\u0434\u0443\u044E\u0449\u0435\u0433\u043E
      \u041D\u0415 \u043F\u043E\u043C\u043E\u0436\u0435\u0442 \u043C\u043D\u0435
      \u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0438\u0442\u044C \u0444\u0438\u0448\u0438\u043D\u0433\u043E\u0432\u044B\u0439
      \u0441\u0430\u0439\u0442?"
    correctAnswerVariant: "\u0412\u0441\u0451 \u0438\u0437 \u043F\u0435\u0440\u0435\u0447\u0438\u0441\u043B\u0435\u043D\u043D\u043E\u0433\u043E
      \u043C\u043E\u0436\u0435\u0442 \u043F\u043E\u043C\u043E\u0447\u044C."
    incorrectAnswerVariants:
    - answer: "\u0410\u0434\u0440\u0435\u0441\u043D\u0430\u044F \u0441\u0442\u0440\u043E\u043A\u0430
        \u0431\u0440\u0430\u0443\u0437\u0435\u0440\u0430."
      clarification: "\u041D\u0435\u0441\u043C\u043E\u0442\u0440\u044F \u043D\u0430
        \u0442\u043E, \u0447\u0442\u043E \u0444\u0438\u0448\u0438\u043D\u0433\u043E\u0432\u044B\u0439
        \u0441\u0430\u0439\u0442 \u0431\u0443\u0434\u0435\u0442 \u0438\u043C\u0435\u0442\u044C
        \u043E\u0442\u043B\u0438\u0447\u043D\u044B\u0439 \u043E\u0442 \u043D\u0430\u0441\u0442\u043E\u044F\u0449\u0435\u0433\u043E
        \u0441\u0430\u0439\u0442\u0430 URL, \u044D\u0442\u043E \u043D\u0435 \u0435\u0434\u0438\u043D\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0439
        \u043F\u0440\u0438\u0437\u043D\u0430\u043A."
    - answer: "\u041E\u0440\u0444\u043E\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043A\u0438\u0435
        \u043E\u0448\u0438\u0431\u043A\u0438."
      clarification: "\u041D\u0435\u0441\u043C\u043E\u0442\u0440\u044F \u043D\u0430
        \u0442\u043E, \u0447\u0442\u043E \u043E\u0448\u0438\u0431\u043A\u0438 \u043E\u0444\u043E\u0440\u043C\u043B\u0435\u043D\u0438\u044F
        \u0432\u0441\u0442\u0440\u0435\u0447\u0430\u044E\u0442\u0441\u044F \u043D\u0430
        \u0441\u0430\u0439\u0442\u0430\u0445 \u043C\u043E\u0448\u0435\u043D\u043D\u0438\u043A\u043E\u0432,
        \u044D\u0442\u043E \u043D\u0435 \u0435\u0434\u0438\u043D\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0439
        \u043F\u0440\u0438\u0437\u043D\u0430\u043A."
  - text: "\u041C\u043D\u0435 \u043F\u0440\u0438\u0448\u043B\u043E \u043F\u0438\u0441\u044C\u043C\u043E
      \u0434\u043B\u044F \u043E\u0431\u043D\u043E\u0432\u043B\u0435\u043D\u0438\u044F
      \u043F\u0430\u0440\u043E\u043B\u044F, \u0445\u043E\u0442\u044F \u044F \u043D\u0435
      \u0437\u0430\u043F\u0440\u0430\u0448\u0438\u0432\u0430\u043B\u0430. \u0414\u043E\u043B\u0436\u043D\u0430
      \u043B\u0438 \u044F \u043E\u0431\u043D\u043E\u0432\u0438\u0442\u044C \u0435\u0433\u043E,
      \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u0443\u044F \u0441\u0441\u044B\u043B\u043A\u0443
      \u0432 \u043F\u0438\u0441\u044C\u043C\u0435?"
    correctAnswerVariant: "\u041D\u0438 \u0432 \u043A\u043E\u0435\u043C \u0441\u043B\u0443\u0447\u0430\u0435.
      \u041F\u0440\u043E\u0438\u0433\u043D\u043E\u0440\u0438\u0440\u0443\u0439\u0442\u0435
      \u043F\u0438\u0441\u044C\u043C\u043E."
    incorrectAnswerVariants:
    - answer: "\u0414\u0430, \u043E\u0431\u044F\u0437\u0430\u0442\u0435\u043B\u044C\u043D\u043E.
        \u0412\u0434\u0440\u0443\u0433 \u0447\u0442\u043E-\u0442\u043E \u0441\u043B\u0443\u0447\u0438\u043B\u043E\u0441\u044C?"
      clarification: "\u0424\u0438\u0448\u0438\u043D\u0433\u043E\u0432\u044B\u0439
        \u0441\u0430\u0439\u0442 \u043C\u043E\u0436\u0435\u0442 \u0432\u044B\u0433\u043B\u044F\u0434\u0435\u0442\u044C
        \u043E\u0447\u0435\u043D\u044C \u0434\u043E\u0441\u0442\u043E\u0432\u0435\u0440\u043D\u043E,
        \u0412\u044B \u0434\u0430\u0436\u0435 \u043D\u0435 \u043F\u043E\u0439\u043C\u0451\u0442\u0435,
        \u0447\u0442\u043E \"\u043E\u0442\u0434\u0430\u043B\u0438\" \u0441\u0432\u043E\u0439
        \u043F\u0430\u0440\u043E\u043B\u044C \u0437\u043B\u043E\u0443\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u0438\u043A\u0430\u043C."
    - answer: "\u0414\u0430, \u043D\u043E \u0442\u043E\u043B\u044C\u043A\u043E \u0435\u0441\u043B\u0438
        \u0441\u0441\u044B\u043B\u043A\u0430 \u0432 \u043F\u0438\u0441\u044C\u043C\u0435
        \u043D\u0435 \u043F\u0435\u0440\u0435\u0432\u043E\u0434\u0438\u0442 \u0412\u0430\u0441
        \u043D\u0430 \u043F\u043E\u0434\u043E\u0437\u0440\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0439
        \u0441\u0430\u0439\u0442."
      clarification: "\u0424\u0438\u0448\u0438\u043D\u0433\u043E\u0432\u0430\u044F
        \u0441\u0441\u044B\u043B\u043A\u0430 \u043C\u043E\u0436\u0435\u0442 \u043F\u0435\u0440\u0435\u043D\u0430\u043F\u0440\u0430\u0432\u043B\u044F\u0442\u044C
